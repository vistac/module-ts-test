{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-empty": "off",
    "no-unused-vars": "off",
    "prefer-const": "warn"
  }
}
